package pibo;

import java.util.ArrayList;

public class Pibonaji {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a=0, b=1 ;
		ArrayList<Integer> al = new ArrayList<>();	
		
		for(int i =0; ; i++){
				
			al.add(b);
			a=al.get(i);
			b+=a;
						
			if(i == 100)break;
		}
		System.out.println(b);

	}

}
