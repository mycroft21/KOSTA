package montecarlo;

import java.util.Scanner;

public class Pingpong {

	public static void main(String[] args) {

		int x=0, y=1,z=+1, n=1;
		n = new Scanner(System.in).nextInt();

		//	7,17,27,37,47,57,
		//	7.14.21.28.35.42.
		
		for(int i =1; i!=n; i++){
			if(i%7==0){z=z*-1;}
			y=y+z;
			System.out.println(i+","+y);
			
		
		
		}
		System.out.println(y);
		
	}

}
