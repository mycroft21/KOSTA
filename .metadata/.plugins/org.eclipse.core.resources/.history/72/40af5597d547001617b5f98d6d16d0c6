package random;

import java.util.Random;

public class Ex01_random {
	public static void main(String[] args) {
		Random r = new Random();
		int[] iNum = { 0, 0, 0, 0, 0, 0 };

	
		for(int i =0; i <6; i++){
			for (int j = 0; j < i; j++) {
				if(iNum[i]==iNum[j]){
					i--;
			}}}
		
		for (int item : iNum) {
			System.out.println(item);
		}

	}
}
