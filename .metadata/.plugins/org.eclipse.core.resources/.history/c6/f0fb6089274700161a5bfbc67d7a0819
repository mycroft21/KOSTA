package dat09_Array;

public class Selection_sort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] arr = { 1, 50, 6, 80, 10, 9 };
		int temp = 0;

		for (int i = 0; i < arr.length; i++) {

			for (int j = i + 1; j < arr.length; j++) {
				if (arr[i] < arr[j]) {
					temp = arr[i]; 
					arr[i] = arr[j];
					arr[j] = temp;
				}
			}
		}

		for(int item : arr){
			System.out.println(arr + "\t");
		}
		
		
	}

}
